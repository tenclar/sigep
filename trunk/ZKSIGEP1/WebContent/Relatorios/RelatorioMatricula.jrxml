	<parameter name="idAluno" class="java.lang.Long"/>
	<parameter name="anoletivoAluno" class="java.lang.Long"/>
	<queryString>
		<![CDATA[SELECT
     a."paialuno" AS a_paialuno,
     a."maealuno" AS a_maealuno,
     p."nome" AS p_nome,
     p."sexo" AS p_sexo,
     p."nascimento" AS p_nascimento,
     p."naturalidade" AS p_naturalidade,
     p."ufnascimento" AS p_ufnascimento,
     p."nacionalidade" AS p_nacionalidade,
     p."necessidade" AS p_necessidade,
     e."logradouro" AS e_logradouro,
     e."numero" AS e_numero,
     e."referencia" AS e_referencia,
     e."bairro" AS e_bairro,
     e."cidade" AS e_cidade,
     e."uf" AS e_uf,
     e."cep" AS e_cep,
     p."telefone" AS p_telefone,
     p."email" AS p_email,
     a."ramalresponsavel" AS a_ramalresponsavel,
     a."nascmae" AS a_nascmae,
     a."nascpai" AS a_nascpai,
     a."profissao" AS a_profissao,
     a."responsavel" AS a_responsavel,
     a."telefoneresponsavel" AS a_telefoneresponsavel,
     p."ramaltelefone" AS p_ramaltelefone,
     a."foto" AS a_foto,
     r."numeroregistro" AS r_numeroregistro,
     r."numerolivro" AS r_numerolivro,
     r."nomecartorio" AS r_nomecartorio,
     r."cidadereg" AS r_cidadereg,
     r."ufreg" AS r_ufreg,
     p."cpf" AS p_cpf,
     p."rg" AS p_rg,
     p."orgexpedidor" AS p_orgexpedidor,
     p."uforgao" AS p_uforgao,
     p."rgexpedicao" AS p_rgexpedicao,
     m."codigomec" AS m_codigomec,
     m."cre" AS m_cre,
     al."ano" AS al_ano,
     s."descricao" AS s_descricao,
     t."nome" AS t_nome,
     t."turno" AS t_turno,
     t."modalidade" AS t_modalidade,
     a."datacadastro" AS a_datacadastro,
     a."etnia" AS a_etnia,
     a."recomendacoesmedicas" AS a_recomendacoesmedicas,
     ne."nivel" AS ne_nivel
FROM
     "aluno" a INNER JOIN "pessoa" p ON a."pessoa_id" = p."id"
     INNER JOIN "endereco" e ON p."endereco" = e."id"
     INNER JOIN "matricula" m ON a."pessoa_id" = m."idaluno"
     INNER JOIN "registro" r ON a."registro" = r."id"
     INNER JOIN "turma" t ON m."turma" = t."id"
     INNER JOIN "serie" s ON t."serie" = s."id"
     INNER JOIN "anoletivo" al ON t."anoletivo" = al."id"
     INNER JOIN "nivelescolar" ne ON s."nivelescolar" = ne."id"
WHERE
     a.pessoa_id = $P{idAluno}
     and t.anoletivo = $P{anoletivoAluno}]]>
	</queryString>
