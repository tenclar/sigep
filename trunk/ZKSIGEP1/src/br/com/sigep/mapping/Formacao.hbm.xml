<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="br.com.sigep.bean">
	<class name="Formacao" lazy="true">
		<id name="id" >
			<generator class="native"></generator>
			<!-- <param name="sequence">id_formacao</param> Criar a sequence das tabelas -->
		</id>
		<property name="titulo" />
		<property name="curso" />
		<property name="anoConclusao" />
		<property name="instituicao" />
		<property name="especializacao" />
	</class>
</hibernate-mapping>