<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="br.com.sigep.bean">
	<class name="Registro" lazy="true">
		<id name="id" >
			<generator class="native"></generator>
			<!-- <param name="sequence">id_registro</param> Criar a sequence das tabelas -->
		</id>
		<property name="numeroRegistro" />
		<property name="numeroLivro" />
		<property name="nomeCartorio" />
		<property name="cidadeReg" />
		<property name="ufReg" />
	</class>
</hibernate-mapping>